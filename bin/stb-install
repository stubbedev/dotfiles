#!/usr/bin/env zsh

function info {
  echo "WELCOME TO THE STUBBEDEV DOTFILES INSTALLER"
  SCRIPT_DIR="$(dirname "$(dirname "$(readlink -f "$0")")")"
  export SCRIPT_DIR
  source "$SCRIPT_DIR/bin/install-funcs"
  source "$SCRIPT_DIR/src/zsh/funcs"
  mkdir -p "$HOME/.ssh/"
  installer_prompt
}

info
