#!/bin/zsh
export STUBBE_CONFIG_ROOT="$HOME/.stubbe"
# PATHS
source "$STUBBE_CONFIG_ROOT/src/zsh/paths"
# FUNCTIONS
source "$STUBBE_CONFIG_ROOT/src/zsh/funcs"
# ALIASES
source "$STUBBE_CONFIG_ROOT/src/zsh/aliases"
# CONSTANTS
source "$STUBBE_CONFIG_ROOT/src/zsh/exports"
# PLUGINS
source "$STUBBE_CONFIG_ROOT/src/zsh/plugins"
# ZSH PARAMS
source "$STUBBE_CONFIG_ROOT/src/zsh/settings"
# SECRETS
touch "$STUBBE_CONFIG_ROOT/src/zsh/secrets"
source "$STUBBE_CONFIG_ROOT/src/zsh/secrets"
