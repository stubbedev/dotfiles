#!/bin/bash
STBZSHSOURCES=(
  "paths"
  "funcs"
  "aliases"
  "settings"
  "secrets"
  "plugins"
)
if [ -d "$HOME/.stubbe" ]; then
    STBDIR="$HOME/.stubbe/src/zsh"
    for SZP in "${STBZSHSOURCES[@]}"; do
        if [ -f "$STBDIR/$SZP" ]; then
            source "$STBDIR/$SZP"
        fi
    done
fi
